.rs{display:none}
/* custom full-screen styles */
:-webkit-full-screen .main {background-color: #ccc;font-size: 24px;width: 95%;}
:-moz-full-screen .main {background-color: #ccc;font-size: 24px;width: 95%;}
:-webkit-full-screen .fs{display:none}
:-moz-full-screen .fs{display:none}
:-webkit-full-screen .rs{display:block}
:-moz-full-screen .rs{display:block}
.slider-popup-slideshow .slider_control{ left: 0; position: fixed;width: 95px; z-index:1000;height: 100%; cursor:pointer; background:#0a1115;}
.slider-popup-slideshow .slider_control .buttons_control{ color: #ffffff;cursor: pointer;display: block;font-size: 40px;height: 65px;padding: 0;text-align: center;width: 100%; }
.slider-popup-slideshow .slider_control .buttons_control:hover{color: #E8817F; background:#ffffff; border-left:6px solid #E8817F; cursor: pointer;display: block;font-size: 40px;height: 65px;padding: 0;text-align: center;width: 100%; }
.slider-popup-slideshow .slider_control .active{ background:#ffffff; color:#E8817F; border-left:6px solid #E8817F;text-align: center;display: block;  }
.slider-popup-slideshow .slider_control .close_button{ background:none; color:#ffffff; border-left:none;text-align: center;display: block;}
.slider-popup-slideshow .slider_control .play_button{ color: #ffffff;cursor: pointer;display: block;font-size: 40px;height: 65px;padding: 0;text-align: center;width: 100%; }
.slider-popup-slideshow span.close{ text-align:right; height:62px; width:62px; opacity:0.50!important; position:relative; z-index:9;}
.slider-popup-slideshow span.close:hover{ opacity:1!important;}
.slider-popup-slideshow span.close img{ max-height:100%; max-width:100%;}
.slider-popup-slideshow #supersized-loader { position:absolute; top:50%; left:50%; z-index:0; width:60px; height:60px; margin:-30px 0 0 -30px; text-indent:-999em; background:url(../images/progress.gif) no-repeat center center;}
.slider-popup-slideshow #supersized {  display:block; position:fixed; left:0; top:0; overflow:hidden; z-index:-999; height:100%; width:100%; }
.slider-popup-slideshow #supersized img { width:auto; height:auto; position:relative; display:none; outline:none; border:none; }
.slider-popup-slideshow #supersized.speed img { -ms-interpolation-mode:nearest-neighbor; image-rendering: -moz-crisp-edges; }	
.slider-popup-slideshow #supersized.quality img { -ms-interpolation-mode:bicubic; image-rendering: optimizeQuality; }			
.slider-popup-slideshow #supersized li { display:block; list-style:none; z-index:-30; position:fixed; overflow:hidden; top:0; left:0; width:100%; height:100%; background:#111; }
.slider-popup-slideshow #supersized a { width:100%; height:100%; display:block; }
.slider-popup-slideshow #supersized li.prevslide { z-index:-20; }
.slider-popup-slideshow #supersized li.activeslide { z-index:-10; }
.slider-popup-slideshow #supersized li.image-loading { background:#111 url(../images/progress.gif) no-repeat center center; width:100%; height:100%; }
.slider-popup-slideshow #supersized li.image-loading img{ visibility:hidden; }
.slider-popup-slideshow #supersized li.prevslide img, #supersized li.activeslide img{ display:inline; }
.slider-popup-slideshow #supersized img{ max-height:100%!important; max-width:auto!important;}

/* Controls Bar----------------------------*/
.slider-popup-slideshow #controls-wrapper { margin:0 auto; height:42px; width:100%; bottom:0px; left:0; z-index:4;  position:fixed; }
.slider-popup-slideshow #controls {height: 100%;overflow: hidden;position: relative;text-align: center;width: 100%; z-index: 5;}
.slider-popup-slideshow #slidecounter { float:left; color:#999; font:14px "Helvetica Neue", Helvetica, Arial, sans-serif; text-shadow:#000 0 -1px 0; margin:0px 10px 0 15px; line-height:42px; }
.slider-popup-slideshow #slidecaption { color: #ffffff; font-family: amplify_personal_use_onlyRg; font-size: 88px; line-height:normal; margin: 0;overflow: hidden;position: fixed;top: 60px; text-align:center; width:100%;}
.slider-popup-slideshow #navigation { float:right; margin:0px 20px 0 0; }
.slider-popup-slideshow #play-button:hover{ background-position:0 1px; cursor:pointer; }
.slider-popup-slideshow #prevslide, .slider-popup-slideshow #nextslide{ height: 90px;margin-top: -21px;opacity: 0.6; position: absolute; top: 40%;width: 50px;}
.slider-popup-slideshow #prevslide{ left:138px; background:url('../images/back.png'); background-size:cover; }
.slider-popup-slideshow #nextslide{ right:42px; background:url('../images/forward.png');  background-size:cover;}
.slider-popup-slideshow #prevslide:active, .slider-popup-slideshow #nextslide:active{ margin-top:-19px; }
.slider-popup-slideshow #prevslide:hover, .slider-popup-slideshow #nextslide:hover{ cursor:pointer; }
.slider-popup-slideshow ul#slide-list{ display: inline-block;position:fixed;padding: 0 0;bottom:70px;text-align: center; width: 80%; left:50%; }
.slider-popup-slideshow #slide-list{  background:url('../images/thumbnail_line.png') ; background-position:center 22px; background-repeat:no-repeat; }
.slider-popup-slideshow ul#slide-list li{ list-style:none; width:20px; height:20px; margin:0 0 0 0; position:absolute; margin-left:-10px;  }
.slider-popup-slideshow ul#slide-list:before { position:absolute; top:9px; height:2px; background:#fff; width:100%; left:0; content:"";}
.slider-popup-slideshow ul#slide-list li.slide-link-0 { left:0;}
.slider-popup-slideshow ul#slide-list li.slide-link-1 { left:25%;}
.slider-popup-slideshow ul#slide-list li.slide-link-2 { left:50%;}
.slider-popup-slideshow ul#slide-list li.slide-link-3 { left:75%;}
.slider-popup-slideshow ul#slide-list li.slide-link-4 { right:0;}
.slider-popup-slideshow ul#slide-list li.current-slide a,
.slider-popup-slideshow ul#slide-list li.current-slide a:hover{ background-position:0 0px;}
.slider-popup-slideshow .slide-link-4{ width:1%!important;}
.slider-popup-slideshow ul#slide-list li a{ display:block; width:20px; height:20px; background:#ffffff; border-radius:50%;}
.slider-popup-slideshow .current-slide a{ display:block; margin-top:-3px; width:30px!important; height:30px!important; background:#ffffff; border-radius:50%;-webkit-transition: all 500ms ease;-moz-transition: all 500ms ease;-ms-transition: all 500ms ease;-o-transition: all 500ms ease;transition: all 500ms ease;}
.slider-popup-slideshow ul#slide-list li a:hover{  cursor:pointer; }
.slider-popup-slideshow #tray-button{ float:right; margin-top:1px; border-left:1px solid #333; background:url('../images/bg-hover.png') repeat-x 0 44px; }
.slider-popup-slideshow #tray-button:hover{ background-position:0 1px; cursor:pointer; }
/* Thumbnail Navigation----------------------------*/	

/* Progress Bar----------------------------*/					
.slider-popup-slideshow #progress-back{ z-index:5; position:fixed; bottom:0; left:0; height:8px; width:100%; background:url('../images/progress-back.png') repeat-x; }
.slider-popup-slideshow #progress-bar{ position:relative; height:8px; width:100%; background:url('../images/progress-bar.png') repeat-x; }
/* Thumbnail Navigation----------------------------*/	

/* Thumbnail Navigation----------------------------*/	
.slider-popup-slideshow #nextthumb,#prevthumb { z-index:2; display:none; position:fixed; bottom:61px; height:75px; width:100px; overflow:hidden; background:#ddd; border:1px solid #fff; -webkit-box-shadow:0 0 5px #000; }
.slider-popup-slideshow #nextthumb { right:12px; }
.slider-popup-slideshow #prevthumb { left:12px; }
.slider-popup-slideshow #nextthumb img, #prevthumb img { width:150px; height:auto;  }
.slider-popup-slideshow #nextthumb:active, #prevthumb:active { bottom:59px; }
.slider-popup-slideshow #nextthumb:hover, #prevthumb:hover { cursor:pointer; }
/* Thumbnail Navigation----------------------------*/	

/* Thumbnail Tray----------------------------*/			
.slider-popup-slideshow #thumb-tray{ position:fixed; z-index:3; bottom:0; left:0; background:url(../images/bg-black.png); height:150px; width:100%; overflow:hidden; text-align:center; -moz-box-shadow: 0px 0px 4px #000; -webkit-box-shadow: 0px 0px 4px #000; box-shadow: 0px 0px 4px #000; }
.slider-popup-slideshow #thumb-back, #thumb-forward{ position:absolute; z-index:5; bottom:42px; height:108px; width:40px; }
.slider-popup-slideshow #thumb-back{ left:0; background: url('../images/thumb-back.png') no-repeat center center;}
.slider-popup-slideshow #thumb-forward{ right:0; background:url('../images/thumb-forward.png') no-repeat center center;}
.slider-popup-slideshow #thumb-back:hover, #thumb-forward:hover{ cursor:pointer; background-color:rgba(256,256,256, 0.1); }
.slider-popup-slideshow #thumb-back:hover{ border-right:1px solid rgba(256,256,256, 0.2); }
.slider-popup-slideshow #thumb-forward:hover{ border-left:1px solid rgba(256,256,256, 0.2); }
.slider-popup-slideshow ul#thumb-list{ display:inline-block; list-style:none; position:relative; left:0px; padding:0 0px; }
.slider-popup-slideshow ul#thumb-list li{ background:#111; list-style:none; display:inline; width:150px; height:108px; overflow:hidden; float:left; margin:0; }
.slider-popup-slideshow ul#thumb-list li img { width:200px; height:auto; opacity:0.5; -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)"; filter:alpha(opacity=60); -webkit-transition: all 100ms ease-in-out; -moz-transition: all 100ms ease-in-out; -o-transition: all 100ms ease-in-out; -ms-transition: all 100ms ease-in-out; transition: all 100ms ease-in-out; }
.slider-popup-slideshow ul#thumb-list li.current-thumb img, ul#thumb-list li:hover img{ opacity:1; -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; filter:alpha(opacity=100); }
.slider-popup-slideshow ul#thumb-list li:hover{ cursor:pointer; }
